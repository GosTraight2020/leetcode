class Solution {
public:
    int fib(int n) {
        if(n<2) return n;
        int tmp1=0, tmp2=0, res=1;
        for(int i=2;i<=n;i++){
            tmp1=tmp2;
            tmp2=res;
            res=(tmp1+tmp2) % 1000000007;
        }
        return res;
    }
};

##################################################
1 初始化 0 0 1 res放在最后
2 循环值 i=2 i<=n
